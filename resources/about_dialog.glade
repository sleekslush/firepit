    <property name="type_hint">dialog</property>
    <property name="program_name">Fire Pit</property>
    <property name="version">0.0</property>
    <property name="copyright" translatable="yes">Copyright (c) 2011 Craig Slusher</property>
    <property name="comments" translatable="yes">A modest client for Campfire chat rooms.</property>
    <property name="website">https://github.com/sleekslush/firepit</property>
    <property name="website_label" translatable="yes">Fork me on Github</property>
